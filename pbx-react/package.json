{
    "name": "react-pbx",
    "version": "1.0.0",
    "author": "ECTL",
    "private": true,
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "find-unused-sass-variables": "find-unused-sass-variables ./src",
        "test": "jest",
        "generate": "plop",
        "g": "yarn run generate",
        "lint:styles": "stylelint 'src/**/*.s?(a|c)ss' --color --cache --cache-location './node_modules/.cache/'",
        "lint:styles:fix": "stylelint 'src/**/*.s?(a|c)ss' --color --fix --cache --cache-location './node_modules/.cache/'",
        "lint:eslint": "eslint './' --quiet --color --cache --cache-location './node_modules/.cache/' --cache-strategy content",
        "lint:eslint:fix": "eslint './' --quiet --color --fix --cache --cache-location './node_modules/.cache/' --cache-strategy content",
        "lint:json": "jsonlint --quiet 'src/assets/locales/en/translation.json'",
        "lint": "yarn run lint:json && yarn prettier --check . && yarn run lint:eslint && yarn run lint:styles",
        "lint:fix": "yarn prettier --write . && yarn run lint:eslint:fix && yarn run lint:styles:fix",
        "build": "NODE_ENV=production yarn encore prod",
        "b": "yarn run build",
        "build-dev": "yarn encore dev",
        "watch": "yarn encore dev --watch",
        "w": "yarn run watch",
        "server": "REACT_REFRESH=1 NODE_ENV=development yarn encore dev-server --mode=development --port 9001",
        "serve": "yarn run server",
        "start": "yarn run server",
        "s": "yarn run server",
        "cache:clear": "rm -r ./node_modules/.cache/*",
        "cc": "yarn run cache:clear"
    },
